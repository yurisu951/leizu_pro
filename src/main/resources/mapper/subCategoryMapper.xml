<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chloe.leizu_pro.mapper.SubCategoryMapper">

    <select id="getSubcategoryListByCategory" resultType="com.chloe.leizu_pro.bean.SubCategory">
        select id, sub_category_name from tbl_sub_category where category = #{category}
    </select>


</mapper>